program Estoque;

uses
  Vcl.Forms,
  uCadastroProduto in 'uCadastroProduto.pas' {ufrmCadProdutos},
  dmConexao in 'dmConexao.pas' {DataModule1: TDataModule},
  uConsultaBase in 'uConsultaBase.pas' {ufrmConsulta};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TufrmCadProdutos, ufrmCadProdutos);
  Application.CreateForm(TDataModule1, DataModule1);
  Application.CreateForm(TufrmConsulta, ufrmConsulta);
  Application.Run;
end.
